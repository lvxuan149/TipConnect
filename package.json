{
  "name": "tipconnect-skeleton",
  "private": true,
  "version": "0.1.0",
  "packageManager": "pnpm@9.12.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "dev:up": "tsx scripts/diagnose.ts && drizzle-kit generate && drizzle-kit migrate && tsx scripts/seed.ts && next dev",
    "test": "vitest -r",
    "test:contract": "vitest -t contract",
    "test:idempotency": "vitest -t idempotency",
    "test:e2e": "vitest -t e2e",
    "dev:clean": "lsof -ti:3000-3010 | xargs kill -9 || true && pnpm dev"
  },
  "dependencies": {
    "@neondatabase/serverless": "^0.10.4",
    "drizzle-orm": "^0.36.0",
    "framer-motion": "^12.23.24",
    "next": "14.2.10",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "swr": "^2.3.6",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.31.5",
    "eslint": "^9.12.0",
    "postcss": "^8.4.45",
    "tailwindcss": "^3.4.14",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "vitest": "^2.1.3"
  }
}
